class ArrayPrint {
    public static void main(String[] args) 
{
        // Declare and initialize the array
        int[] numbers = {10, 20, 30, 40, 50};
        // Print all elements using a loop
        for (int i = 0; i < numbers.length; i++) 
        {
            System.out.println(numbers[i]);
        }
    }
}
