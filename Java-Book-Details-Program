public class Main {
    public static void main(String[] args) {
        Book b = new Book();
        b.setData("Java Programming", 450.50);
        b.showData();
    }
}

class Book {
    String title;   double price;
    void setData(String t, double p) {
        title = t; price = p;
    }

    void showData() {
        System.out.println("Book Title: " + title);
        System.out.println("Book Price: " + price);
    }
}
